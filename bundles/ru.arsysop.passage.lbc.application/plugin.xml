<?eclipse version="3.4"?>
<plugin>
 <extension
       id="ru.arsysop.passage.lbc.application"
       point="org.eclipse.core.runtime.applications">
    <application
          cardinality="1"
          thread="main"
          visible="true">
    </application>
 </extension>
 <extension
       id="product"
       point="org.eclipse.core.runtime.products">
    <product
          application="ru.arsysop.passage.lbc.application"
          name="Passage Licensing Backend Component Server">
       <property
             name="appName"
             value="Passage Licensing Backend Component Server">
       </property>
    </product>
 </extension>
</plugin>
